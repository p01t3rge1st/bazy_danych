CREATE VIEW AvailableClassesView AS
            SELECT
                c.Class_ID,
                s.Subject_Name,
                l.First_Name AS Lecturer_First_Name,
                l.Last_Name AS Lecturer_Last_Name,
                b.Address AS Building_Address,
                r.Room_ID,
                c.Start_Time,
                c.End_Time,
                res.Reservation_Date
            FROM Class c
            JOIN Lecturer l ON c.Lecturer_ID = l.Lecturer_ID
            JOIN Subject s ON c.Subject_ID = s.Subject_ID
            JOIN Room r ON c.Room_ID = r.Room_ID
            JOIN Building b ON r.Building_ID = b.Building_ID
            LEFT JOIN Reservation res ON c.Class_ID = res.Class_ID
            WHERE c.Is_Cancelled = 0</sql><current_tab id="1"/></tab_sql></sqlb_project>
